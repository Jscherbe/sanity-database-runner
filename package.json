{
  "name": "@ulu/sanity-database-runner",
  "version": "1.0.0",
  "description": "A CLI tool to run scripts for backing up and migrating a Sanity.io database.",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "sanity-db-run": "bin/cli.js"
  },
  "files": [
    "lib",
    "bin",
    "types",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "test": "node tests/01-happy-path.js && node tests/02-error-handling.js",
    "types": "npx tsc",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "eslint ."
  },
  "keywords": [
    "sanity",
    "database",
    "migration",
    "runner",
    "cli"
