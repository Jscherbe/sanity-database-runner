  "author": "Joe Scherben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jscherbe/sanity-database-runner.git"
  },
  "bugs": {
    "url": "https://github.com/Jscherbe/sanity-database-runner/issues"
  },
  "homepage": "https://github.com/Jscherbe/sanity-database-runner#readme",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "prompts": "^2.4.2",
    "which": "^6.0.0"
  },
  "devDependencies": {
    "@ulu/utils": "^0.0.34",
    "@eslint/js": "^9.7.0",
    "docdash": "^2.0.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "jsdoc": "^4.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@sanity/client": ">=6"
  }
